{{#each this}}
  <div class='photo-card' data-url='{{this.largeImageURL}}'>

    <img src='{{this.webformatURL}}' alt='{{this.tags}}' loading='lazy' />
    <div class='info'>
      <p class='info-item'>
        <b>Likes:</b>
        <span>{{this.likes}}</span>
      </p>
      <p class='info-item'>
        <b>Views:</b>
        <span>{{this.views}}</span>
      </p>
      <p class='info-item'>
        <b>Comments:</b>
        <span>{{this.comments}}</span>
      </p>
      <p class='info-item'>
        <b>Downloads:</b>
        <span>{{this.downloads}}</span>
      </p>
    </div>

  </div>
{{/each}}